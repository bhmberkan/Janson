
@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <title>Bhmberkan Admin Dashboard</title>
    <!-- Favicon icon -->
    <link rel="icon" type="image/png" sizes="16x16" href="~/adminweb/images/favicon.png">
    <!-- Pignose Calender -->
    <link href="~/adminweb//plugins/pg-calendar/css/pignose.calendar.min.css" rel="stylesheet">
    <!-- Chartist -->
    <link rel="stylesheet" href="~/adminweb//plugins/chartist/css/chartist.min.css">
    <link rel="stylesheet" href="~/adminweb//plugins/chartist-plugin-tooltips/css/chartist-plugin-tooltip.css">
    <!-- Custom Stylesheet -->
    <link href="~/adminweb/css/style.css" rel="stylesheet">

</head>

<body>

    <!--*******************
        Preloader start
    ********************-->
    <div id="preloader">
        <div class="loader">
            <svg class="circular" viewBox="25 25 50 50">
                <circle class="path" cx="50" cy="50" r="20" fill="none" stroke-width="3" stroke-miterlimit="10" />
            </svg>
        </div>
    </div>
    <!--*******************
        Preloader end
    ********************-->
    <!--**********************************
        Main wrapper start
    ***********************************-->
    <div id="main-wrapper">

        <!--**********************************
            Nav header start
        ***********************************-->
        <div class="nav-header">
            <div class="brand-logo">
                <a href="index.html">
                    <b class="logo-abbr"><img src="images/logo.png" alt=""> </b>
                    <span class="logo-compact"><img src="./images/logo-compact.png" alt=""></span>
                    <span class="brand-title">
                        <img src="images/logo-text.png" alt="">
                    </span>
                </a>
            </div>
        </div>
        <!--**********************************
            Nav header end
        ***********************************-->
        <!--**********************************
            Header start
        ***********************************-->
       @Html.Action("AdminHeader","Admin")
      
        @Html.Action("SideBarPartial","Admin")
      
    <div class="content-body">

        <div class="container-fluid" >
            <div class="row" >
                <div class="col-12"">
                    <div class="card" style="color:rgb(83, 2, 2)">
                        <div class="card-body">
                            <p>@RenderBody()</p>
                            <br />

                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- #/ container -->
    </div>
        <!--**********************************
            Content body end
        ***********************************-->
        <!--**********************************
            Footer start
        ***********************************-->
        <div class="footer">
            <div class="copyright">
                <p>Bhm &copy; Designed & Developed by <a href="https://www.instagram.com/bhmberkan/">Bhmberkan</a> 2025</p>
            </div>
        </div>
        <!--**********************************
            Footer end
        ***********************************-->
    </div>
    <!--**********************************
        Main wrapper end
    ***********************************-->
    <!--**********************************
        Scripts
    ***********************************-->
    <script src="/adminweb/plugins/common/common.min.js"></script>
    <script src="/adminweb/js/custom.min.js"></script>
    <script src="/adminweb/js/settings.js"></script>
    <script src="/adminweb/js/gleek.js"></script>
    <script src="/adminweb/js/styleSwitcher.js"></script>

    <!-- Chartjs -->
    <script src="/adminweb/plugins/chart.js/Chart.bundle.min.js"></script>
    <!-- Circle progress -->
    <script src="/adminweb/plugins/circle-progress/circle-progress.min.js"></script>
    <!-- Datamap -->
    <script src="/adminweb/plugins/d3v3/index.js"></script>
    <script src="/adminweb/plugins/topojson/topojson.min.js"></script>
    <script src="/adminweb/plugins/datamaps/datamaps.world.min.js"></script>
    <!-- Morrisjs -->
    <script src="/adminweb/plugins/raphael/raphael.min.js"></script>
    <script src="/adminweb/plugins/morris/morris.min.js"></script>
    <!-- Pignose Calender -->
    <script src="/adminweb/plugins/moment/moment.min.js"></script>
    <script src="/adminweb/plugins/pg-calendar/js/pignose.calendar.min.js"></script>
    <!-- ChartistJS -->
    <script src="/adminweb/plugins/chartist/js/chartist.min.js"></script>
    <script src="/adminweb/plugins/chartist-plugin-tooltips/js/chartist-plugin-tooltip.min.js"></script>



    <script src="~/adminweb/js/dashboard/dashboard-1.js"></script>

</body>

</html>