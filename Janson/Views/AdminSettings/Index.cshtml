@using Janson.Models.Entity
@model List<AdminTbl>


@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Settings From Admin</h2>
<br />



<table class="table table-bordered form-group" style="background-color: rgb(36, 113, 163); border-radius: 10px; font-family: Arial; font-size: 15px;">
    <tr>

        <th>Kullanıcı</th>
        <th>Şifre</th>
       
        <th>Güncelle</th>

        <th>Sil</th>

    </tr>
    @foreach (var x in Model)
    {
        <tr>

            <td>@x.Kullanıcı</td>
            <td>@x.Sifre</td>
         



            <td><a href="/AdminSettings/UpdateSettingPage/@x.ID" class="btn mb-1 btn-rounded btn-outline-dark">Güncelle</a></td>
            <td><a href="/AdminSettings/DeleteSettingPage/@x.ID" class="btn mb-1 btn-rounded btn-outline-dark">Sil</a></td>

        </tr>

    }
</table>




<a href="/AdminSettings/InsertSettingPage/" class="btn  btn-outline-info">Ekle</a>
