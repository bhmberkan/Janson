@using Janson.Models.Entity
@model List<BlogTBL>


<aside class="single_sidebar_widget tag_cloud_widget">
    <h4 class="widget_title" style="color: #2d2d2d;">Tag Clouds</h4>
    <ul class="list">
        @foreach (var Tag in Model.Select(x => x.Tag).Distinct())
        {

            <li>
                <a href="#">@Tag</a>
            </li>


        }
    </ul>
    </aside>